<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="najtek.database.mapper.user.UserMapper">
	<select id="selectById" parameterType="najtek.infra.user.User" resultType="najtek.infra.user.User">
		select * from user where id = #{id}
	</select>
	<select id="selectByUsername" parameterType="najtek.infra.user.User" resultType="najtek.infra.user.User">
		select * from user where username = #{username}
	</select>
</mapper>
